                <tr>
                    <th>S.N</th>
                    <th>Course Name</th>
                    <th>Course Fee</th>
                   
                    <th>Delete</th>
                </tr>
                
                <%
                if(userData.length!=0){
                var i=1;
                userData.forEach(function(data){
                %>
                <tr>
                    <td><%=i; %></td>
                    <td><%=data.course %></td>
                    <td><%=data.fee %></td>
                
                    <td>   <button onclick="myJavascriptFunction(data.id)" type="button">Delete</button></td>
                    <td><a href="/addmission/edit/<%= data.course%>">Delete</a></td>
                </tr>
                <%  i++; }) %>
                <% } else{ %>
                    <tr>
                        <td colspan="7">No Data Found</td>
                    </tr>
                <% } %>
            </table>
            </div>
</body>

</html>
